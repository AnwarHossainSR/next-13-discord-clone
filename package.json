{
  "name": "next-js-boilerplate",
  "version": "0.1.0",
  "author": "Anwar Hossain",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "devt": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "eslint": "next lint --fix --dir src",
    "prettier": "prettier --write .",
    "lint": "yarn eslint && yarn prettier",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "postbuild": "next-sitemap && yarn prisma generate",
    "prepare": "husky install",
    "postinstall": "yarn prisma generate"
  },
  "lint-staged": {
    "*.{js,jsx,md,ts,tsx,html,css,scss}": "yarn prettier",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix --cache-location ./node_modules/.cache/.eslintcache"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.23.2",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@hookform/resolvers": "^3.3.0",
    "@livekit/components-react": "^1.1.3",
    "@livekit/components-styles": "^1.0.6",
    "@next/bundle-analyzer": "^13.4.19",
    "@prisma/client": "^5.2.0",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tanstack/react-query": "^4.33.0",
    "@uploadthing/react": "^5.3.0",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "config": "^3.3.9",
    "date-fns": "^2.30.0",
    "emoji-mart": "^5.5.2",
    "livekit-client": "^1.13.0",
    "livekit-server-sdk": "^1.2.6",
    "lucide-react": "^0.268.0",
    "next": "13.4.19",
    "next-themes": "^0.2.1",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "uploadthing": "^5.3.3",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@next/eslint-plugin-next": "^13.4.19",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
